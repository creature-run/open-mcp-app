{
  "name": "items-example",
  "version": "0.1.0",
  "description": "Example MCP App demonstrating SDK patterns",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:ui": "vite build --watch",
    "build": "npm run build:ui && npm run build:server",
    "build:ui": "vite build --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "open-mcp-app": "0.0.7",
    "nanoid": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "postcss": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-singlefile": "^2.0.0"
  }
}
