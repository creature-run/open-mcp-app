{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "functions": {
    "api/mcp.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/.well-known/oauth-authorization-server",
      "destination": "/api/.well-known/oauth-authorization-server"
    }
  ]
}
