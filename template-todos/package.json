{
  "name": "mcp-template-todos",
  "version": "0.0.1",
  "description": "An MCP App for managing todos with a clean, interactive UI.",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite build --watch\" \"nodemon -q --ext ts --watch src/server --exec 'npx tsx src/server/lib/dev.ts'\"",
    "build": "npm run build:ui && npm run build:server",
    "build:ui": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server/index.js"
  },
  "creature": {
    "name": "todos",
    "publish": true
  },
  "dependencies": {
    "@creature-ai/sdk": "file:/Users/m/projects/n/creature/desktop/artifacts/sdk",
    "mongodb": "^6.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.24.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/creature-run/creature.git",
    "directory": "template-todos"
  },
  "homepage": "https://creature.run/docs",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-singlefile": "^2.0.3"
  }
}
